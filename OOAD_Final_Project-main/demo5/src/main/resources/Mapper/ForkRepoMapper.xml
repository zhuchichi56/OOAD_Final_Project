<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.demo.mapper.ForkRepoMapper">
    <insert id="forkRepo">
        insert into agentforkrepo values (#{agentName}, #{repoId});
    </insert>



    <delete id="removeFork">
        delete from agentforkrepo where agentName = #{agentName} and repositoryId = #{repoId};
    </delete>




</mapper>