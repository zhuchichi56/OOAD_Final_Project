<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.demo.mapper.WatchRepoMapper">
    <insert id="watchRepo">
        insert into agentwatchrepo values (#{agentName}, #{repoId});
    </insert>


    <delete id="removeWatch">
        delete from agentwatchrepo where agentName = #{agentName} and repositoryId = #{repoId};
    </delete>




</mapper>